#  SERV_ATTR  产品实例附加属性
   SERV_ID              NUMBER(14)                      not null,
   AGREEMENT_ID         NUMBER(14)                      not null,
   ATTR_ID              NUMBER(9)                       not null,
   ATTR_VAL             VARCHAR2(1024)                    not null,
   EFF_DATE             DATE                            not null,
   EXP_DATE             DATE