#  PRICING_COMBINE  定价组合
   PRICING_COMBINE_ID   		NUMBER(9)                       not null,
   PRICING_PLAN_ID      		NUMBER(9)                       not null,
   EVENT_PRICING_STRATEGY_ID 	NUMBER(9)                  		not null,
   LIFE_CYCLE_ID        		NUMBER(9)                       not null,
   OFFER_OBJECT_ID      		NUMBER(9),
   CALC_PRIORITY        		NUMBER(3)                       not null,
   constraint PK_PRICING_COMBINE primary key (PRICING_COMBINE_ID)