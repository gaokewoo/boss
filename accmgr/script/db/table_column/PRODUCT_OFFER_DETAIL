#	销售品明细	PRODUCT_OFFER_DETAIL
	OFFER_ID               NUMBER(9),    
	ELEMENT_TYPE           VARCHAR2(3),  
	ELEMENT_ID             NUMBER(14),   
	ELEMENT_CODE           NUMBER(9),    
	OFFER_DETAIL_ROLE_CD   VARCHAR2(30), 
	ELEMENT_DESC           VARCHAR2(250)	NOT NULL,
	OBJECT_AMOUNT_START    NUMBER(8)        NOT NULL,
	OBJECT_AMOUNT_END      NUMBER(8)        NOT NULL,
	ORDER_DEAL_FLAG        NUMBER(5),    
	BILL_FLAG              NUMBER(5),    
	MEM_PRIORITY           NUMBER(3),    
	IS_PRICING_OBJ         CHAR(1),      
	STATE                  VARCHAR2(3),  
	CREATE_DATE            DATE,         
	STATE_DATE             DATE,         
	RULE_ID                NUMBER(9),    
	SEL_FLAG               CHAR(1),
	constraint PK_PRODUCT_OFFER_DETAIL primary key (OFFER_ID,ELEMENT_TYPE,ELEMENT_ID)      