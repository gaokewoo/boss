#  INTEREST_RESPOND  预存回报信息
   INTEREST_INFO_ID     NUMBER(9)                       not null,
   ACCT_BALANCE_ID      NUMBER(14)                       not null,
   CHARGE               NUMBER(12)                      not null,
   "DATE"               DATE                            not null,
   STATE                VARCHAR (3)                     not null,
   constraint PK_INTEREST_RESPOND primary key (INTEREST_INFO_ID)