#  OWE_TYPE_LIMIT  欠费类型限额
   OWE_TYPE_LIMIT_ID    NUMBER(9)                       not null,
   OWE_BUSINESS_TYPE_ID NUMBER(9)                       not null,
   CREDIT_CEIL          NUMBER(5)                       not null,
   CREDIT_FLOOR         NUMBER(5)                       not null,
   QUANTITY             NUMBER(12)                      not null,
   constraint PK_OWE_TYPE_LIMIT primary key (OWE_TYPE_LIMIT_ID)