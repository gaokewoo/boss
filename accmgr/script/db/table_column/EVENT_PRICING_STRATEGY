#  EVENT_PRICING_STRATEGY  事件定价策略
   EVENT_PRICING_STRATEGY_ID 	NUMBER(9)                       not null,
   EVENT_TYPE_ID        		NUMBER(9),
   EVENT_PRICING_STRATEGY_NAME 	VARCHAR2(50)                    not null,
   EVENT_PRICING_STRATEGY_DESC 	VARCHAR2(4000)                  not null,
   EVENT_PRICING_FEETYPES 		VARCHAR2(8),
   EVENT_PRICING_BILLTYPE 		VARCHAR2(1),
   VPNFLAG						CHAR(1),
   constraint PK_EVENT_PRICING_STRATEGY primary key (EVENT_PRICING_STRATEGY_ID)