#	TABLE_SYNC_RELATION		表同步关系对应表
	SOURCE_TABLE    VARCHAR2(50)  NOT NULL,
	SOURCE_COLUMN   VARCHAR2(50),
	DEST_TABLE      VARCHAR2(50)  NOT NULL,
	DEST_COLUMN     VARCHAR2(50)  NOT NULL,
	DATA_TYPE       VARCHAR2(5)   NOT NULL,
	DATA_VALUE      VARCHAR2(50),
	INDEX_FLAG      CHAR(1)       NOT NULL,
	COMB_FLAG       CHAR(1)       NOT NULL 