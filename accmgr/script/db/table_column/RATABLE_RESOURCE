#  RATABLE_RESOURCE  积量类型描述
   RATABLE_RESOURCE_ID  	NUMBER(9)                       not null,
   ORG_TARIFF_UNIT_ID       NUMBER(9)						not null,
   RATABLE_RESOURCE_NAME 	VARCHAR(50)						not null,
   DEFAULT_RATABLE_CYCLE_TYPE_ID 	NUMBER(9),
   CAN_BE_NEGATIVE      	VARCHAR2(1)						not null,
   DURATIVE_TYPE        	VARCHAR2(3)						not null,
   constraint PK_RATABLE_RESOURCE primary key (RATABLE_RESOURCE_ID)