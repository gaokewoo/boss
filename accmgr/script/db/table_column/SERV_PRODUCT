#	附加产品实例	SERV_PRODUCT
	SERV_PRODUCT_ID        NUMBER(14), 
	SERV_ID                NUMBER(14) 		NOT NULL,
	AGREEMENT_ID           NUMBER(14)       NOT NULL,
	PRODUCT_ID             NUMBER(9)        NOT NULL,
	CREATED_DATE           DATE             NOT NULL,
	EFF_DATE               DATE             NOT NULL,
	EXP_DATE               DATE             NULL,  
	STATE                  VARCHAR2(3)      NOT NULL,
	STOP_RENT_TIME         DATE,       
	BEGIN_RENT_TIME        DATE,
	constraint PK_SERV_PRODUCT primary key (SERV_PRODUCT_ID)       