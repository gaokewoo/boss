# 销售品实例构成	PRODUCT_OFFER_INSTANCE_DETAIL
  INSTANCE_RELATION_ID       NUMBER(14)         NOT NULL,
  PRODUCT_OFFER_INSTANCE_ID  NUMBER(14)			NOT NULL,
  INSTANCE_TYPE              VARCHAR2(3 BYTE)	NOT NULL,
  INSTANCE_ID                NUMBER(14)			NOT NULL,
  STATE                      VARCHAR2(3 BYTE)	NOT NULL,
  EFF_DATE                   DATE				NOT NULL,
  EXP_DATE                   DATE				NOT NULL,
  REL_GROUP_ID               NUMBER(12),					
  REL_PROD_INST_ID           NUMBER(14),
  REL_OFFER_INST_ID          NUMBER(14),					
  OFFER_MBR_ROLE_CD          VARCHAR2(30 BYTE),
  constraint PK_PRODUCT_OFFER_INSTANCE_DET primary key (INSTANCE_RELATION_ID)