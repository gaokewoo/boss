#	BILLING_TABLE_MONITOR
   	TABLE_NAME      VARCHAR2(50),
	FIELD_NAME      VARCHAR2(50),
	DOMAIN          VARCHAR2(50),
	SERIAL          NUMBER(5) default 0,
	DOMAIN_EXP      CHAR(1),
	ALARM_THRESHOLD NUMBER(5),
	ALARM_EXP       CHAR(1),
	constraint PK_TABLE_MONITOR primary key (TABLE_NAME,FIELD_NAME,DOMAIN,SERIAL)