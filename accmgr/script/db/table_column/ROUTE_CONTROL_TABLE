#	ROUTE_CONTROL_TABLE
  	ROUTE_NAME       VARCHAR2(15 BYTE)            NOT NULL,
  	SYSTEM_TYPE      CHAR(8 BYTE)                 NOT NULL,
  	ROUTE_TYPE       CHAR(2 BYTE)                 NOT NULL,
  	ROUTE_GROUP      VARCHAR2(8 BYTE)             NOT NULL,
  	ROUTE_FLAG       CHAR(1 BYTE)                 NOT NULL,
  	ROUTE_IP         VARCHAR2(15 BYTE)            NOT NULL,
  	ROUTE_PORT       NUMBER(6)                    NOT NULL,
  	QUERY_IP         VARCHAR2(15 BYTE)            NOT NULL,
  	QUERY_PORT       NUMBER(6)                    NOT NULL,
  	CHANGE_DATE      DATE                         NOT NULL,
  	SYSTEM_TYPE_NEW  CHAR(8 BYTE)                 NOT NULL,
  	ROUTE_GROUP_NEW  VARCHAR2(8 BYTE)             NOT NULL,
  	ROUTE_IP_NEW     VARCHAR2(15 BYTE)            NOT NULL,
  	ROUTE_PORT_NEW   NUMBER(6)                    NOT NULL,
  	QUERY_IP_NEW     VARCHAR2(15 BYTE)            NOT NULL,
  	QUERY_PORT_NEW   NUMBER(6)                    NOT NULL,
  	INSTANCE         VARCHAR2(16 BYTE)            NOT NULL,
  	SYNC_ID          VARCHAR2(8 BYTE)             NOT NULL,
  	NOTE             VARCHAR2(50 BYTE),
  	AUDIT_FLAG       CHAR(1 BYTE)                 NOT NULL,
  	constraint PK_ROUTE_CONTROL_TABLE primary key (ROUTE_NAME)