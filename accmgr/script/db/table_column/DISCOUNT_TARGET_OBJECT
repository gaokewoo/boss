#  DISCOUNT_TARGET_OBJECT  优惠应用对象
   DISCOUNT_EXPRESS_ID  	NUMBER(9)                       not null,
   PRICING_REF_OBJECT_ID 	NUMBER(9)                       not null,
   REPATITION_TYPE_ID   	NUMBER(9),
   constraint PK_DISCOUNT_TARGET_OBJECT primary key (DISCOUNT_EXPRESS_ID, PRICING_REF_OBJECT_ID)