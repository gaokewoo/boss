#/bin/bash

p_num=`$BOSS_HOME/boss/bossstatus 2>&1|grep PROCESS_ID|wc -l`

if [ "$p_num" != "0" ];then
    echo "Servers already started!"
    exit 1
fi

cd $BOSS_HOME/boss/bin
echo "Start BossMonitorServer begin"
./BossMonitorServer
echo "Start BossMonitorServer end"

sleep 1

echo "Start OpenAccountServer begin"
./OpenAccountServer
echo "Start OpenAccountServer end"

echo "Start PayFeeServer begin"
./PayFeeServer
echo "Start PayFeeServer  end"

echo "Start FetchNbrServer begin"
./FetchNbrServer 
echo "Start FetchNbrServer end"


