#  SERV_IDENTIFICATION_DEAD  主产品标识
   SERV_ID              NUMBER(14)                      not null,
   AGREEMENT_ID         NUMBER(14)                       not null,
   ACC_NBR              VARCHAR2(30)                    not null,
   EFF_DATE             DATE                            not null,
   EXP_DATE             DATE,
   dead_time			DATE,
	dead_staff_id		NUMBER(9),
	dead_optsn			NUMBER(14),
   constraint PK_SERV_IDENTIFICATION_DEAD primary key (SERV_ID, AGREEMENT_ID)