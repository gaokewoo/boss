create table detail_item_voice
(
system_type VARCHAR(2) not null,
record_type VARCHAR(2) not null,
call_type VARCHAR(2) not null,
imsi VARCHAR(15) not null,
imei VARCHAR(20) not null,
msisdn VARCHAR(15) not null,
other_party VARCHAR(24) not null,
start_datetime date not null,
call_duration	NUMBER(6) not null,
msc VARCHAR(10) not null,
msrn VARCHAR(11) not null,
lac VARCHAR(5) not null,
cell_id VARCHAR(5) not null,
other_lac VARCHAR(5) not null,
other_cell_id VARCHAR(5) not null,
trunk_groupout VARCHAR(16) not null,
trunk_groupin VARCHAR(16) not null,
service_type VARCHAR(3) not null,
service_code VARCHAR(4) not null,
hot_flag VARCHAR(1) not null,
status VARCHAR(7) not null,
service_attr VARCHAR(20) not null,
info_num VARCHAR(3) not null,
calling_group_num VARCHAR(16) not null,
calling_short_num VARCHAR(7) not null,
called_group_num VARCHAR(16) not null,
called_short_num VARCHAR(7) not null,
block_flag VARCHAR(1) not null,
call_flag VARCHAR(1) not null,
home_area_code VARCHAR(4) not null,
visit_area_code VARCHAR(4) not null,
other_home_code VARCHAR(4) not null,
other_visit_code VARCHAR(4) not null,
called_code VARCHAR(4) not null,
user_type VARCHAR(1) not null,
roam_type VARCHAR(1) not null,
fee_type VARCHAR(1) not null,
dial_type VARCHAR(3) not null,
chat_type VARCHAR(3) not null,
fee1 NUMBER(10) not null,
fee2 NUMBER(10) not null,
fee3 NUMBER(10) not null,
old_fee1 NUMBER(10) not null,
old_fee2 NUMBER(10) not null,
old_fee3 NUMBER(10) not null,
info_fee1 VARCHAR(60) not null,
info_fee2 VARCHAR(60) not null,
info_fee3 VARCHAR(60) not null,
file_name VARCHAR(40) not null,
deal_time VARCHAR(12) not null,
group_id VARCHAR(15) not null,
product_code VARCHAR(8) not null,
a_number VARCHAR(24) not null,
brand_code VARCHAR(2) not null,
roam_type_query VARCHAR(1) not null,
fee_type_query VARCHAR(1) not null,
dealfile   VARCHAR(40) not null,
error_code VARCHAR(20) not null,
favour_fee1 VARCHAR(10) not null,
favour_fee2 VARCHAR(10) not null,
favour_fee3 VARCHAR(10) not null,
favour_flag VARCHAR(1) not null
);


