#	参考值定义	REF_VALUE
	REF_VALUE_ID           		NUMBER(9)		not null,   
	REF_VALUE_TYPE              VARCHAR2(3) 	not null,
	VALUE_TYPE                  VARCHAR2(3) 	not null,
	PRICING_REF_OBJECT_ID       NUMBER(9),   
	VALUE_STRING                VARCHAR2(30),
	PRICING_PRARM_ID            NUMBER(9),   
	RATE_PRECISION              NUMBER(5)		not null,   
	CALC_PRECISION              NUMBER(5)		not null,
	constraint PK_REF_VALUE primary key (REF_VALUE_ID)   