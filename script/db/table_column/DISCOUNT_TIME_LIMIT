#  DISCOUNT_TIME_LIMIT  优惠时间限制
   DISCOUNT_TIME_LIMIT_ID 	NUMBER(9)                       not null,
   DISCOUNT_EXPRESS_ID  	NUMBER(9)                       not null,
   DISCOUNT_CYCLE_TYPE  	VARCHAR2(3)                     not null,
   BEGIN_TIME_TYPE      	VARCHAR2(3)                     not null,
   BEGIN_CALC_OBJECT    	NUMBER(9),
   BEGIN_TIME_DURATION  	NUMBER(12,5),
   END_TIME_TYPE        	VARCHAR2(3)                     not null,
   END_CALC_OBJECT      	NUMBER(9),
   END_TIME_DURATION    	NUMBER(12,5),
   constraint PK_DISCOUNT_TIME_LIMIT primary key (DISCOUNT_TIME_LIMIT_ID)