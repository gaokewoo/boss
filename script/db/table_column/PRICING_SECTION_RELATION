#  PRICING_SECTION_RELATION  定价段落关系
   A_SECTION_ID         NUMBER(9)                       not null,
   Z_SECTION_ID         NUMBER(9)                       not null,
   RELATION_TYPE        VARCHAR2(3)                     not null,
   constraint PK_PRICING_SECTION_RELATION primary key (A_SECTION_ID, Z_SECTION_ID)