#  TARIFF_UNIT  资费单位描述
   TARIFF_UNIT_ID       NUMBER(9)                       not null,
   MEASURE_METHOD_ID    NUMBER(9)                       not null,
   TARIFF_UNIT_NAME     VARCHAR2(50)                    not null,
   STANDARD_CONVERSION_RATE NUMBER(12,5)                    not null,
   constraint PK_TARIFF_UNIT primary key (TARIFF_UNIT_ID)