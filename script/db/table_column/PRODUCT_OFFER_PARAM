#	销售品参数	PRODUCT_OFFER_PARAM
	OFFER_PARAM_ID      NUMBER(9)   	NOT NULL,
	OFFER_ID            NUMBER(9)       NOT NULL,
	PARAM_ATTR_ID       NUMBER(9)       NOT NULL,
	PARAM_NAME          VARCHAR2(50)    NOT NULL,
	DEFAULT_VALUE       VARCHAR2(30),
	constraint PK_PRODUCT_OFFER_PARAM primary key (OFFER_PARAM_ID)