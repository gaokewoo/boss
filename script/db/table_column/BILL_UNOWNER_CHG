#	计费无主增量表（ BILL_UNOWNER_CHG ）
	BILL_UNOWNER_ID				NUMBER(14),	
	ACC_NBR           		VARCHAR2(30)   	NOT NULL,
	UNOWNER_TYPE      		VARCHAR2(8)             ,
	REGION_CODE       		VARCHAR2(10)   	NOT NULL,
	IMSI_NO           		VARCHAR2(32)            ,
	UNOWNER_FEE       		NUMBER(12)		NOT NULL,
	UNOWNER_COUNT     		NUMBER(4)		        ,
	UNOWNER_BEGIN_TIME		DATE			NOT NULL,
	UNOWNER_LAST_TIME 		DATE			NOT NULL,
	INSERT_TIME       		DATE			NOT NULL,
	STATE             		VARCHAR2(3) 	NOT NULL,
	STATE_DATE        		DATE			NOT NULL,
	OP_TYPE					VARCHAR2(1) 	NOT NULL,
	DEAL_FLAG				VARCHAR2(1) 	NOT NULL,
	constraint PK_BILL_UNOWNER_CHG primary key (BILL_UNOWNER_ID) 