   ORG_ID               NUMBER(12)                      not null,
   CREDIT_CODE          VARCHAR2(2)                     not null,
   CREDIT_VALUE         NUMBER(12)                    not null,
   USED_CREDIT          NUMBER(12)                    not null,
   OP_ACCEPT            NUMBER(14)                      not null,
